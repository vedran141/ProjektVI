<Page navigatingTo="onNavigatingTo" xmlns="http://schemas.nativescript.org/tns.xsd">
    <ActionBar>
        <Label text="Senzori VI" />
    </ActionBar>

    <GridLayout rows="30, 30, 30" columns="20,*,20">
        <Label text="{{ x }}" row="0" col="1" backgroundColor="#00A7FF"/>
        <Label text="{{ y }}" row="1" col="1" backgroundColor="#FF0073"/>
        <Label text="{{ z }}" row="2" col="1" backgroundColor="#E6F106"/>
    </GridLayout>
</Page>
